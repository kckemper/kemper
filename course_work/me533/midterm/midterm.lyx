\begin_inset Formula \begin{eqnarray*}
\dot{\tilde{x}} & = & \dot{x}-\dot{\hat{x}}\\
\dot{\tilde{\theta}} & = & \dot{\theta}-\dot{\hat{\theta}}\end{eqnarray*}

\end_inset


\begin_inset Formula \begin{eqnarray*}
\dot{\tilde{x}} & = & \left(f\left(x\right)\theta+u\right)-\left(f\left(x\right)\hat{\theta}+u+\tilde{x}\right)\\
\dot{\tilde{\theta}} & = & -\tilde{x}f\left(x\right)\end{eqnarray*}

\end_inset


\begin_inset Formula \begin{eqnarray*}
\dot{\tilde{x}} & = & f\left(x\right)\theta+u-f\left(x\right)\hat{\theta}-u-\tilde{x}\\
\dot{\tilde{\theta}} & = & -\tilde{x}f\left(x\right)\end{eqnarray*}

\end_inset


\begin_inset Formula \begin{eqnarray*}
\dot{\tilde{x}} & = & f\left(x\right)\theta-f\left(x\right)\hat{\theta}-\tilde{x}\\
\dot{\tilde{\theta}} & = & -\tilde{x}f\left(x\right)\end{eqnarray*}

\end_inset


\begin_inset Formula \begin{eqnarray*}
\dot{\tilde{x}} & = & f\left(x\right)\left(\theta-\hat{\theta}\right)-\tilde{x}\\
\dot{\tilde{\theta}} & = & -\tilde{x}f\left(x\right)\end{eqnarray*}

\end_inset


\begin_inset Formula \begin{eqnarray*}
\dot{\tilde{x}} & = & f\left(x\right)\tilde{\theta}-\tilde{x}\\
\dot{\tilde{\theta}} & = & -\tilde{x}f\left(x\right)\end{eqnarray*}

