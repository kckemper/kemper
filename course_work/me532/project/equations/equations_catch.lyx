\begin_layout Standard
First find the Laplace of 
\begin_inset Formula $F_{m}(t)$
\end_inset

:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
\textrm{Set the motor to constant max torque:}\\
F_{m}(t) & = & F_{mc}u(t)\\
F_{m}(s) & = & \frac{F_{mc}}{s}\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
Now solve for 
\begin_inset Formula $X_{L}(t)$
\end_inset

:
\begin_inset Formula \begin{eqnarray*}
\frac{F_{m}(s)+X_{L}(s)\left(Bs+k\right)+x_{m0}B-x_{L0}B+m_{m}x_{m0}s+m_{m}\dot{x}_{m0}}{m_{m}s^{2}+Bs+k} & = & \frac{X_{L}(s)\left(m_{L}s^{2}+Bs+k\right)-m_{L}x_{L0}s-m_{L}\dot{x}_{L0}+x_{m0}B-x_{L0}B}{Bs+k}\\
\frac{F_{m}(s)+X_{L}(s)\left(Bs+k\right)+x_{m0}B-x_{L0}B+m_{m}x_{m0}s+m_{m}\dot{x}_{m0}}{m_{m}s^{2}+Bs+k} & = & X_{L}(s)\frac{\left(m_{L}s^{2}+Bs+k\right)}{Bs+k}+\frac{-m_{L}x_{L0}s-m_{L}\dot{x}_{L0}+x_{m0}B-x_{L0}B}{Bs+k}\\
\frac{F_{m}(s)+X_{L}(s)\left(Bs+k\right)+x_{m0}B-x_{L0}B+m_{m}x_{m0}s+m_{m}\dot{x}_{m0}}{m_{m}s^{2}+Bs+k}+\frac{m_{L}x_{L0}s+m_{L}\dot{x}_{L0}-x_{m0}B+x_{L0}B}{Bs+k} & = & X_{L}(s)\frac{\left(m_{L}s^{2}+Bs+k\right)}{Bs+k}\\
\left(Bs+k\right)\frac{F_{m}(s)+X_{L}(s)\left(Bs+k\right)+x_{m0}B-x_{L0}B+m_{m}x_{m0}s+m_{m}\dot{x}_{m0}}{m_{m}s^{2}+Bs+k}+m_{L}x_{L0}s+m_{L}\dot{x}_{L0}-x_{m0}B+x_{L0}B & = & X_{L}(s)\left(m_{L}s^{2}+Bs+k\right)\\
\left(\frac{Bs+k}{m_{L}s^{2}+Bs+k}\right)\frac{F_{m}(s)+X_{L}(s)\left(Bs+k\right)+x_{m0}B-x_{L0}B+m_{m}x_{m0}s+m_{m}\dot{x}_{m0}}{m_{m}s^{2}+Bs+k}+\frac{m_{L}x_{L0}s+m_{L}\dot{x}_{L0}-x_{m0}B+x_{L0}B}{m_{L}s^{2}+Bs+k} & = & X_{L}(s)\\
\\\\\\\frac{F_{m}(s)+X_{L}(s)\left(Bs+k\right)+x_{m0}B-x_{L0}B+m_{m}x_{m0}s+m_{m}\dot{x}_{m0}}{m_{m}s^{2}+Bs+k} & = & \frac{X_{L}(s)\left(m_{L}s^{2}+Bs+k\right)-m_{L}x_{L0}s-m_{L}\dot{x}_{L0}+x_{m0}B-x_{L0}B}{Bs+k}\\
\left(Bs+k\right)\left(F_{m}(s)+X_{L}(s)\left(Bs+k\right)+x_{m0}B-x_{L0}B+m_{m}x_{m0}s+m_{m}\dot{x}_{m0}\right) & = & \left(X_{L}(s)\left(m_{L}s^{2}+Bs+k\right)-m_{L}x_{L0}s-m_{L}\dot{x}_{L0}+x_{m0}B-x_{L0}B\right)\left(m_{m}s^{2}+Bs+k\right)\\
F_{m}(s)\left(Bs+k\right)+X_{L}(s)\left(Bs+k\right)^{2}+\left(x_{m0}B-x_{L0}B+m_{m}x_{m0}s+m_{m}\dot{x}_{m0}\right)\left(Bs+k\right) & = & X_{L}(s)\left(m_{L}s^{2}+Bs+k\right)\left(m_{m}s^{2}+Bs+k\right)+\left(-m_{L}x_{L0}s-m_{L}\dot{x}_{L0}+x_{m0}B-x_{L0}B\right)\left(m_{m}s^{2}+Bs+k\right)\\
F_{m}(s)\left(Bs+k\right)+\left(x_{m0}B-x_{L0}B+m_{m}x_{m0}s+m_{m}\dot{x}_{m0}\right)\left(Bs+k\right)+\left(m_{L}x_{L0}s+m_{L}\dot{x}_{L0}-x_{m0}B+x_{L0}B\right)\left(m_{m}s^{2}+Bs+k\right) & = & X_{L}(s)\left(m_{L}s^{2}+Bs+k\right)\left(m_{m}s^{2}+Bs+k\right)-X_{L}(s)\left(Bs+k\right)^{2}\\
F_{m}(s)\left(Bs+k\right)+\left(x_{m0}B-x_{L0}B+m_{m}x_{m0}s+m_{m}\dot{x}_{m0}\right)\left(Bs+k\right)+\left(m_{L}x_{L0}s+m_{L}\dot{x}_{L0}-x_{m0}B+x_{L0}B\right)\left(m_{m}s^{2}+Bs+k\right) & = & X_{L}(s)\left(\left(m_{L}s^{2}+Bs+k\right)\left(m_{m}s^{2}+Bs+k\right)-\left(Bs+k\right)^{2}\right)\\
\frac{F_{m}(s)\left(Bs+k\right)+\left(x_{m0}B-x_{L0}B+m_{m}x_{m0}s+m_{m}\dot{x}_{m0}\right)\left(Bs+k\right)+\left(m_{L}x_{L0}s+m_{L}\dot{x}_{L0}-x_{m0}B+x_{L0}B\right)\left(m_{m}s^{2}+Bs+k\right)}{\left(m_{L}s^{2}+Bs+k\right)\left(m_{m}s^{2}+Bs+k\right)-\left(Bs+k\right)^{2}} & = & X_{L}(s)\\
X_{L}(s) & = & F_{m}(s)\frac{\left(Bs+k\right)}{\left(m_{L}s^{2}+Bs+k\right)\left(m_{m}s^{2}+Bs+k\right)-\left(Bs+k\right)^{2}}+\frac{\left(x_{m0}B-x_{L0}B+m_{m}x_{m0}s+m_{m}\dot{x}_{m0}\right)\left(Bs+k\right)}{\left(m_{L}s^{2}+Bs+k\right)\left(m_{m}s^{2}+Bs+k\right)-\left(Bs+k\right)^{2}}+\frac{\left(m_{L}x_{L0}s+m_{L}\dot{x}_{L0}-x_{m0}B+x_{L0}B\right)\left(m_{m}s^{2}+Bs+k\right)}{\left(m_{L}s^{2}+Bs+k\right)\left(m_{m}s^{2}+Bs+k\right)-\left(Bs+k\right)^{2}}\end{eqnarray*}

\end_inset


\begin_inset Formula \begin{eqnarray*}
\text{Now sub in \ensuremath{F_{m}(s)}and inverse laplace:}\\
X_{L}(s) & = & \left(\frac{F_{mc}}{s}\right)\frac{Bs+k}{\left(m_{L}s^{2}+Bs+k\right)\left(m_{m}s^{2}+Bs+k\right)-\left(Bs+k\right)^{2}}+\frac{\left(x_{m0}B-x_{L0}B+m_{m}x_{m0}s+m_{m}\dot{x}_{m0}\right)\left(Bs+k\right)}{\left(m_{L}s^{2}+Bs+k\right)\left(m_{m}s^{2}+Bs+k\right)-\left(Bs+k\right)^{2}}+\frac{\left(m_{L}x_{L0}s+m_{L}\dot{x}_{L0}-x_{m0}B+x_{L0}B\right)\left(m_{m}s^{2}+Bs+k\right)}{\left(m_{L}s^{2}+Bs+k\right)\left(m_{m}s^{2}+Bs+k\right)-\left(Bs+k\right)^{2}}\\
\\\\\\b_{1} & = & \sqrt{\left(m_{L}+m_{m}\right)\left(-4km_{L}m_{m}+B^{2}\left(m_{L}+m_{m}\right)\right)}\\
\\a_{1} & = & \frac{\left(B\left(m_{L}+m_{m}\right)+b_{1}\right)}{2m_{L}m_{m}}\\
a_{2} & = & \frac{b_{1}}{m_{L}m_{m}}\\
\\\\x_{L}(t) & = & \frac{F_{mc}e^{-a_{1}t}\left(B\left(-1+e^{a_{2}t}\right)m_{L}m_{m}\left(m_{L}+m_{m}\right)+b_{1}\left(\left(1+e^{a_{2}t}\right)m_{L}m_{m}+e^{a_{1}t}\left(-2m_{L}m_{m}+k\left(m_{L}+m_{m}\right)t^{2}\right)\right)\right)}{2k\left(m_{L}+m_{m}\right)^{5/2}\sqrt{-4km_{L}m_{m}+B^{2}\left(m_{L}+m_{m}\right)}}\\
 &  & +\frac{e^{-a_{1}t}\left(2\dot{x}_{m0}m_{m}\left(-\left(-1+e^{a_{2}t}\right)m_{L}m_{m}+e^{a_{1}t}b_{1}t\right)-\left(1+e^{a_{2}t}-2e^{a_{1}t}\right)m_{m}b_{1}x_{m0}+B\left(-2e^{a_{1}t}b_{1}t\left(x_{L0}-x_{m0}\right)+\left(-1+e^{a_{2}t}\right)m_{L}m_{m}\left(2x_{L0}-x_{m0}\right)-m_{m}^{2}x_{m0}+e^{a_{2}t}m_{m}^{2}x_{m0}\right)\right)}{2\left(m_{L}+m_{m}\right)^{3/2}\sqrt{-4km_{L}m_{m}+B^{2}\left(m_{L}+m_{m}\right)}}\\
 &  & +\frac{e^{-a_{1}t}\left(2\dot{x}_{L0}m_{L}\left(\left(-1+e^{a_{2}t}\right)m_{m}^{2}+e^{a_{1}t}b_{1}t\right)+\left(2e^{a_{1}t}m_{L}+m_{m}+e^{a_{2}t}m_{m}\right)b_{1}x_{L0}+B\left(-\left(-1+e^{a_{2}t}\right)m_{L}m_{m}x_{L0}+\left(-1+e^{a_{2}t}\right)m_{m}^{2}\left(x_{L0}-2x_{m0}\right)+2e^{a_{1}t}b_{1}t\left(x_{L0}-x_{m0}\right)\right)\right)}{2\left(m_{L}+m_{m}\right)^{3/2}\sqrt{-4km_{L}m_{m}+B^{2}\left(m_{L}+m_{m}\right)}}\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
Now solve for 
\begin_inset Formula $X_{m}(t)$
\end_inset

:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
\text{From sys1:}\\
X_{m}(s)\left(m_{m}s^{2}+Bs+k\right) & = & F_{m}(s)+X_{L}(s)\left(Bs+k\right)+x_{m0}B-x_{L0}B+m_{m}x_{m0}s+m_{m}\dot{x}_{m0}\\
\text{From sys2:}\\
X_{L}(s)\left(m_{L}s^{2}+Bs+k\right) & = & X_{m}(s)\left(Bs+k\right)+m_{L}x_{L0}s+m_{L}\dot{x}_{L0}-x_{m0}B+x_{L0}B\\
X_{L}(s) & = & \frac{X_{m}(s)\left(Bs+k\right)+m_{L}x_{L0}s+m_{L}\dot{x}_{L0}-x_{m0}B+x_{L0}B}{m_{L}s^{2}+Bs+k}\\
\\\text{Plug \ensuremath{X_{L}}into sys1 eq:}\\
X_{m}(s)\left(m_{m}s^{2}+Bs+k\right) & = & F_{m}(s)+\left(\frac{X_{m}(s)\left(Bs+k\right)+m_{L}x_{L0}s+m_{L}\dot{x}_{L0}-x_{m0}B+x_{L0}B}{m_{L}s^{2}+Bs+k}\right)\left(Bs+k\right)+x_{m0}B-x_{L0}B+m_{m}x_{m0}s+m_{m}\dot{x}_{m0}\\
X_{m}(s)\left(m_{m}s^{2}+Bs+k\right) & = & F_{m}(s)+X_{m}(s)\frac{\left(Bs+k\right)^{2}}{m_{L}s^{2}+Bs+k}+\frac{\left(m_{L}x_{L0}s+m_{L}\dot{x}_{L0}-x_{m0}B+x_{L0}B\right)\left(Bs+k\right)}{m_{L}s^{2}+Bs+k}+x_{m0}B-x_{L0}B+m_{m}x_{m0}s+m_{m}\dot{x}_{m0}\\
X_{m}(s)\left(m_{m}s^{2}+Bs+k\right)-X_{m}(s)\frac{\left(Bs+k\right)^{2}}{m_{L}s^{2}+Bs+k} & = & F_{m}(s)+\frac{\left(m_{L}x_{L0}s+m_{L}\dot{x}_{L0}-x_{m0}B+x_{L0}B\right)\left(Bs+k\right)}{m_{L}s^{2}+Bs+k}+x_{m0}B-x_{L0}B+m_{m}x_{m0}s+m_{m}\dot{x}_{m0}\\
X_{m}(s)\left(\left(m_{m}s^{2}+Bs+k\right)-\frac{\left(Bs+k\right)^{2}}{m_{L}s^{2}+Bs+k}\right) & = & F_{m}(s)+\frac{\left(m_{L}x_{L0}s+m_{L}\dot{x}_{L0}-x_{m0}B+x_{L0}B\right)\left(Bs+k\right)}{m_{L}s^{2}+Bs+k}+x_{m0}B-x_{L0}B+m_{m}x_{m0}s+m_{m}\dot{x}_{m0}\\
X_{m}(s) & = & \frac{F_{m}(s)+\frac{\left(m_{L}x_{L0}s+m_{L}\dot{x}_{L0}-x_{m0}B+x_{L0}B\right)\left(Bs+k\right)}{m_{L}s^{2}+Bs+k}+x_{m0}B-x_{L0}B+m_{m}x_{m0}s+m_{m}\dot{x}_{m0}}{\left(\left(m_{m}s^{2}+Bs+k\right)-\frac{\left(Bs+k\right)^{2}}{m_{L}s^{2}+Bs+k}\right)}\\
X_{m}(s) & = & F_{m}(s)\frac{1}{\left(m_{m}s^{2}+Bs+k\right)-\frac{\left(Bs+k\right)^{2}}{m_{L}s^{2}+Bs+k}}+\frac{\left(m_{L}x_{L0}s+m_{L}\dot{x}_{L0}-x_{m0}B+x_{L0}B\right)\left(Bs+k\right)}{\left(m_{L}s^{2}+Bs+k\right)\left(\left(m_{m}s^{2}+Bs+k\right)-\frac{\left(Bs+k\right)^{2}}{m_{L}s^{2}+Bs+k}\right)}+\frac{x_{m0}B-x_{L0}B+m_{m}x_{m0}s+m_{m}\dot{x}_{m0}}{\left(m_{m}s^{2}+Bs+k\right)-\frac{\left(Bs+k\right)^{2}}{m_{L}s^{2}+Bs+k}}\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
\text{Now sub in \ensuremath{F_{m}(s)}and inverse laplace:}\\
X_{m}(s) & = & \left(\frac{F_{mc}}{s}\right)\frac{1}{\left(m_{m}s^{2}+Bs+k\right)-\frac{\left(Bs+k\right)^{2}}{m_{L}s^{2}+Bs+k}}+\frac{\left(m_{L}x_{L0}s+m_{L}\dot{x}_{L0}-x_{m0}B+x_{L0}B\right)\left(Bs+k\right)}{\left(m_{L}s^{2}+Bs+k\right)\left(\left(m_{m}s^{2}+Bs+k\right)-\frac{\left(Bs+k\right)^{2}}{m_{L}s^{2}+Bs+k}\right)}+\frac{x_{m0}B-x_{L0}B+m_{m}x_{m0}s+m_{m}\dot{x}_{m0}}{\left(m_{m}s^{2}+Bs+k\right)-\frac{\left(Bs+k\right)^{2}}{m_{L}s^{2}+Bs+k}}\\
\\\\\\b_{1} & = & \sqrt{\left(m_{L}+m_{m}\right)\left(-4km_{L}m_{m}+B^{2}\left(m_{L}+m_{m}\right)\right)}\\
\\a_{1} & = & \frac{\left(B\left(m_{L}+m_{m}\right)+b_{1}\right)}{2m_{L}m_{m}}\\
a_{2} & = & \frac{b_{1}}{m_{L}m_{m}}\\
