\begin_layout Standard
System 1 (motor side):
\begin_inset Formula \begin{eqnarray*}
\Sigma F=m_{m}\ddot{x}_{m} & = & F_{m}-F_{k}-F_{B}\\
m_{m}\ddot{x}_{m}(t) & = & F_{m}(t)-k\left(x_{m}(t)-x_{L}(t)\right)-B\left(\dot{x}_{m}(t)-\dot{x}_{L}(t)\right)\\
m_{m}\ddot{x}_{m}(t) & = & F_{m}(t)-kx_{m}(t)+kx_{L}(t)-B\dot{x}_{m}(t)+B\dot{x}_{L}(t)\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
Take the Laplace:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
m_{m}\left[s^{2}X_{m}(s)-sx_{m0}-\dot{x}_{m0}\right] & = & F_{m}(s)-k\left(X_{m}(s)-X_{L}(s)\right)-B\left(sX_{m}(s)-x_{m0}-sX_{L}(s)+x_{L0}\right)\\
m_{m}s^{2}X_{m}(s)-m_{m}x_{m0}s-m_{m}\dot{x}_{m0} & = & F_{m}(s)-X_{m}(s)k+X_{L}(s)k-X_{m}(s)Bs+x_{m0}B+X_{L}(s)Bs-x_{L0}B\\
\\\textrm{Move }X_{m}(s)\textrm{ to the left side:}\\
X_{m}(s)m_{m}s^{2}+X_{m}(s)Bs+X_{m}(s)k & = & F_{m}(s)+X_{L}(s)Bs+X_{L}(s)k+x_{m0}B-x_{L0}B+m_{m}x_{m0}s+m_{m}\dot{x}_{m0}\\
X_{m}(s)\left(m_{m}s^{2}+Bs+k\right) & = & F_{m}(s)+X_{L}(s)\left(Bs+k\right)+x_{m0}B-x_{L0}B+m_{m}x_{m0}s+m_{m}\dot{x}_{m0}\\
X_{m}(s) & = & \frac{F_{m}(s)+X_{L}(s)\left(Bs+k\right)+x_{m0}B-x_{L0}B+m_{m}x_{m0}s+m_{m}\dot{x}_{m0}}{m_{m}s^{2}+Bs+k}\end{eqnarray*}

\end_inset


\begin_inset VSpace bigskip
\end_inset


\end_layout

\begin_layout Standard
System 2 (load side):
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
\Sigma F=m_{L}\ddot{x}_{L} & = & F_{k}+F_{B}\\
m_{L}\ddot{x}_{L} & = & k\left(x_{m}(t)-x_{L}(t)\right)+B\left(\dot{x}_{m}(t)-\dot{x}_{L}(t)\right)\\
m_{L}\ddot{x}_{L} & = & x_{m}(t)k-x_{L}(t)k+\dot{x}_{m}(t)B-\dot{x}_{L}(t)B\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
Take the Laplace:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
m_{L}\left[s^{2}X_{L}(s)-sx_{L0}-\dot{x}_{L0}\right] & = & k\left(X_{m}(s)-X_{L}(s)\right)+B\left(sX_{m}(s)-x_{m0}-sX_{L}(s)+x_{L0}\right)\\
m_{L}s^{2}X_{L}(s)-m_{L}x_{L0}s-m_{L}\dot{x}_{L0} & = & X_{m}(s)k-X_{L}(s)k+X_{m}(s)Bs-x_{m0}B-X_{L}(s)Bs+x_{L0}B\\
\\\textrm{Move }X_{m}(s)\textrm{ to the left side:}\\
X_{L}(s)m_{L}s^{2}+X_{L}(s)Bs+X_{L}(s)k-m_{L}x_{L0}s-m_{L}\dot{x}_{L0}+x_{m0}B-x_{L0}B & = & X_{m}(s)k+X_{m}(s)Bs\\
X_{L}(s)\left(m_{L}s^{2}+Bs+k\right)-m_{L}x_{L0}s-m_{L}\dot{x}_{L0}+x_{m0}B-x_{L0}B & = & X_{m}(s)\left(Bs+k\right)\\
X_{m}(s) & = & \frac{X_{L}(s)\left(m_{L}s^{2}+Bs+k\right)-m_{L}x_{L0}s-m_{L}\dot{x}_{L0}+x_{m0}B-x_{L0}B}{Bs+k}\end{eqnarray*}

\end_inset


\end_layout

