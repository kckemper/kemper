\end_deeper
\begin_layout Enumerate
Write the Euler-Lagrange equation (do not solve) for
\begin_inset Formula \[
J\left(f\right)=\int_{a}^{b}\underbrace{\left(f'^{2}+f^{2}\right)}_{L}dx\]

\end_inset


\begin_inset Formula \[
f\left(a\right)=y_{1},\, f\left(b\right)=y_{2}\]

\end_inset


\begin_inset Formula \begin{eqnarray*}
\frac{\partial L}{\partial f}+\frac{d}{dx}\frac{\partial L}{\partial f'} & = & 0\\
2f+\frac{d}{dx}\left(2f'\right) & = & 0\\
2f+2f'' & = & 0\\
f+f'' & = & 0\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Enumerate
Find extremals for:
\begin_inset Formula \[
J\left(f\right)=\int_{0}^{1}\underbrace{\left(\frac{1}{2}f'^{2}+f'f+f'+f\right)}_{L}dx\]

\end_inset


\begin_inset Formula \[
f\left(0\right)=\frac{1}{2},\, f\left(1\right)=?\]

\end_inset


\begin_inset Formula \[
f=f^{*}+\epsilon h\]

\end_inset


\begin_inset Formula \begin{eqnarray*}
\frac{\partial J}{\partial\epsilon}=\int_{0}^{1}\left(\frac{\partial L}{\partial x}\underbrace{\frac{dx}{d\epsilon}}_{0}+\frac{\partial L}{\partial f}\underbrace{\frac{df}{d\epsilon}}_{h}+\frac{\partial L}{\partial f'}\underbrace{\frac{df'}{d\epsilon}}_{h'}\right)dx & = & 0\\
\int_{0}^{1}\left(\frac{\partial L}{\partial f}h+\frac{\partial L}{\partial f'}h'\right)dx & = & 0\end{eqnarray*}

\end_inset


\begin_inset Formula \begin{eqnarray*}
\text{substitution:}\\
u & = & \frac{\partial L}{\partial f'}\\
dv & = & h'dx\\
\\du & = & \frac{d}{dx}\frac{\partial L}{\partial f'}\\
v & = & h\end{eqnarray*}

\end_inset


\begin_inset Formula \begin{eqnarray*}
\int_{0}^{1}\left(\frac{\partial L}{\partial f}-\frac{d}{dx}\frac{\partial L}{\partial f'}\right)hdx+\left.\frac{\partial L}{\partial f'}h\right|_{0}^{1} & = & 0\end{eqnarray*}

\end_inset

Because 
\begin_inset Formula $h\left(1\right)$
\end_inset

 can be anything but 
\begin_inset Formula $h\left(0\right)=0$
\end_inset

 then 
\begin_inset Formula $\left.\frac{\partial L}{\partial f'}\right|_{1}=0$
\end_inset

.
\begin_inset Formula \begin{eqnarray*}
\int_{0}^{1}\left(\frac{\partial L}{\partial f}-\frac{d}{dx}\frac{\partial L}{\partial f'}\right)hdx & = & 0\\
\\\text{by the FLCOV:}\\
\frac{\partial L}{\partial f}-\frac{d}{dx}\frac{\partial L}{\partial f'} & = & 0\end{eqnarray*}

\end_inset


\begin_inset Formula \begin{eqnarray*}
\frac{\partial L}{\partial f} & = & \left(f'+1\right)\\
\frac{\partial L}{\partial f'} & = & \left(f'+f+1\right)\end{eqnarray*}

\end_inset


\begin_inset Formula \begin{eqnarray*}
\left(f'+1\right)-\frac{d}{dx}\left(f'+f+1\right) & = & 0\\
f'+1-f''-f' & = & 0\\
1-f'' & = & 0\\
f'' & = & 1\end{eqnarray*}

\end_inset


\begin_inset Formula \begin{eqnarray*}
\text{FONC:}\\
\left.\frac{\partial L}{\partial f'}\right|_{1} & = & 0\\
f\left(0\right) & = & \frac{1}{2}\\
f'' & = & 1\end{eqnarray*}

\end_inset


\begin_inset Formula \begin{eqnarray*}
f'' & = & 1\\
f' & = & x+a\\
f & = & \frac{x^{2}}{2}+ax+b\end{eqnarray*}

\end_inset


\begin_inset Formula \[
f\left(0\right)=\frac{1}{2}=b\]

\end_inset


\begin_inset Formula \begin{eqnarray*}
\left.\frac{\partial L}{\partial f'}\right|_{1} & = & 0\\
\left.\left(f'+f+1\right)\right|_{1} & = & 0\\
\left.\left(x+a+\frac{x^{2}}{2}+ax+b+1\right)\right|_{1} & = & 0\\
1+a+\frac{1}{2}+a+\frac{1}{2}+1 & = & 0\\
2a & = & -3\\
a & = & -\frac{3}{2}\end{eqnarray*}

\end_inset


\begin_inset Formula \[
f=\frac{x^{2}}{2}-\frac{3}{2}x+\frac{1}{2}\]

\end_inset


\begin_inset Newpage pagebreak
\end_inset


\end_layout

\begin_layout Enumerate
Find the extremals for the isoperimetric problem
\begin_inset Formula \[
J\left(f\right)=\int_{0}^{\pi}\underbrace{\left(f'^{2}\right)}_{L}dx\]

\end_inset


\begin_inset Formula \[
f\left(0\right)=f\left(\pi\right)=0\]

\end_inset

s.t.
\begin_inset Formula \begin{eqnarray*}
\int_{0}^{\pi}\left(f^{2}\right)dx & = & 1\\
\\G=\int_{0}^{\pi}\underbrace{\left(f^{2}\right)}_{g}dx & -1= & 0\end{eqnarray*}

\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula \begin{eqnarray*}
f & = & f^{*}+\epsilon h\\
f' & = & f'^{*}+\epsilon h'\end{eqnarray*}

\end_inset


\begin_inset Formula \begin{eqnarray*}
H\left(f\right) & = & J\left(f\right)+\lambda G\\
 & = & \int_{0}^{\pi}Ldx+\lambda\left(\int_{0}^{\pi}gdx-1\right)\\
H\left(f\right) & = & \int_{0}^{\pi}\left(L+\lambda g\right)dx-\lambda\end{eqnarray*}

\end_inset


\begin_inset Formula \begin{eqnarray*}
\frac{dH}{d\epsilon}=0 & = & \int_{0}^{\pi}\left(\frac{\partial L}{\partial x}\underbrace{\frac{dx}{d\epsilon}}_{0}+\frac{\partial L}{\partial f}\underbrace{\frac{df}{d\epsilon}}_{h}+\frac{\partial L}{\partial f'}\underbrace{\frac{df'}{d\epsilon}}_{h'}+\lambda\left(\frac{\partial g}{\partial x}\underbrace{\frac{dx}{d\epsilon}}_{0}+\frac{\partial g}{\partial f}\underbrace{\frac{df}{d\epsilon}}_{h}+\frac{\partial g}{\partial f'}\underbrace{\frac{df'}{d\epsilon}}_{h'}\right)\right)dx\\
0 & = & \int_{0}^{\pi}\left(\frac{\partial L}{\partial f}h+\frac{\partial L}{\partial f'}h'+\lambda\left(\frac{\partial g}{\partial f}h+\frac{\partial g}{\partial f'}h'\right)\right)dx\\
0 & = & \int_{0}^{\pi}\left(\frac{\partial L}{\partial f}h-\frac{d}{dx}\frac{\partial L}{\partial f'}h+\lambda\left(\frac{\partial g}{\partial f}h-\frac{d}{dx}\frac{\partial g}{\partial f'}h\right)\right)dx+\underbrace{\left.\frac{\partial L}{\partial f'}h\right|_{0}^{\pi}}_{0}+\underbrace{\lambda\left.\frac{\partial g}{\partial f'}h\right|_{0}^{\pi}}_{0}\\
0 & = & \int_{0}^{\pi}\left(\frac{\partial L}{\partial f}-\frac{d}{dx}\frac{\partial L}{\partial f'}+\lambda\left(\frac{\partial g}{\partial f}-\frac{d}{dx}\frac{\partial g}{\partial f'}\right)\right)hdx\end{eqnarray*}

\end_inset

By the FLCOV:
\begin_inset Formula \[
\frac{\partial L}{\partial f}-\frac{d}{dx}\frac{\partial L}{\partial f'}+\lambda\left(\frac{\partial g}{\partial f}-\frac{d}{dx}\frac{\partial g}{\partial f'}\right)=0\]

\end_inset


\begin_inset Formula \begin{eqnarray*}
\frac{\partial L}{\partial f} & = & 0\\
\frac{\partial L}{\partial f'} & = & 2f'\\
\frac{\partial g}{\partial f} & = & 2f\\
\frac{\partial g}{\partial f'} & = & 0\end{eqnarray*}

\end_inset


\begin_inset Formula \begin{eqnarray*}
-\frac{d}{dx}\left(2f'\right)+\lambda\left(2f\right) & = & 0\\
-f''+\lambda f & = & 0\end{eqnarray*}

\end_inset


\begin_inset Formula \begin{eqnarray*}
\text{FONC:}\\
f\left(0\right)=f\left(\pi\right) & = & 0\\
f''-\lambda f & = & 0\\
\int_{0}^{\pi}\left(f^{2}\right)dx & = & 1\end{eqnarray*}

\end_inset


\begin_inset Formula \[
f''-\lambda f=0\]

\end_inset

If 
\begin_inset Formula $\lambda=0$
\end_inset


\begin_inset Formula \begin{eqnarray*}
f'' & = & 0\\
f & = & ax+b\end{eqnarray*}

\end_inset


\begin_inset Formula \begin{eqnarray*}
f\left(0\right) & = & 0\implies b=0\\
f\left(\pi\right) & = & 0\implies a\left(\pi\right)+0=0\rightarrow a=0\end{eqnarray*}

\end_inset


\begin_inset Formula \[
f=0\bot\int_{0}^{\pi}\left(f^{2}\right)dx=1\]

\end_inset

If 
\begin_inset Formula $\lambda>0$
\end_inset


\begin_inset Formula \[
\lambda=\mu^{2}\]

\end_inset


\begin_inset Formula \begin{eqnarray*}
f''-\mu^{2}f & = & 0\\
f & = & e^{rx}\end{eqnarray*}

\end_inset


\begin_inset Formula \begin{eqnarray*}
r^{2}e^{rx}-\mu^{2}e^{rx} & = & 0\\
\left(r^{2}-\mu^{2}\right)e^{rx} & = & 0\\
\\\implies r^{2}-\mu^{2} & = & 0\\
r & = & \pm\mu\end{eqnarray*}

\end_inset


\begin_inset Formula \begin{eqnarray*}
f & = & ae^{\mu x}+be^{-\mu x}\\
f\left(0\right)=0 & = & a+b\implies a=-b\\
f\left(\pi\right)=0 & = & ae^{\mu\pi}+be^{-\mu\pi}\implies a\left(e^{\mu\pi}-e^{-\mu\pi}\right)=0\\
\\\implies\\
\mu & = & 0\\
a & = & 0\end{eqnarray*}

\end_inset


\begin_inset Formula \[
\bot\int_{0}^{\pi}\left(f^{2}\right)dx=1\]

\end_inset

For 
\begin_inset Formula $\lambda<0$
\end_inset


\begin_inset Formula \[
\lambda=-\mu^{2}\]

\end_inset


\begin_inset Formula \begin{eqnarray*}
f''+\mu^{2}f & = & 0\\
f & = & e^{rx}\end{eqnarray*}

\end_inset


\begin_inset Formula \begin{eqnarray*}
r^{2}e^{rx}+\mu^{2}e^{rx} & = & 0\\
\left(r^{2}+\mu^{2}\right)e^{rx} & = & 0\\
\\\implies r^{2}+\mu^{2} & = & 0\\
r & = & \pm i\mu\end{eqnarray*}

\end_inset


\begin_inset Formula \begin{eqnarray*}
f & = & a\cos\left(\mu x\right)+b\sin\left(\mu x\right)\\
f\left(0\right)=0 & = & 0\implies a=0\\
f\left(\pi\right)=0 & = & b\sin\left(\mu\pi\right)\\
\\\implies\\
b & = & 0\\
\sin\left(\mu\pi\right) & = & 0\end{eqnarray*}

\end_inset


\begin_inset Formula \[
\mu=1\]

\end_inset


\begin_inset Formula \begin{eqnarray*}
\int_{0}^{\pi}\left(b\sin x\right)^{2}dx & = & 1\\
b^{2}\underbrace{\int_{0}^{\pi}\left(\sin^{2}x\right)dx}_{\frac{\pi}{2}} & = & 1\\
b^{2} & = & \frac{2}{\pi}\\
b & = & \pm\sqrt{\frac{2}{\pi}}\end{eqnarray*}

\end_inset


\begin_inset Formula \[
f=\sqrt{\frac{2}{\pi}}\sin x\]

\end_inset


