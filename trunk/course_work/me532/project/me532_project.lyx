\begin_inset Formula \begin{eqnarray*}
0 & = & \det\left\{ \lambda I-A\right\} \\
0 & = & \begin{vmatrix}\lambda & -1 & 0 & 0\\
\frac{k}{m_{m}\ell_{s}^{2}} & \lambda & \frac{k}{m_{m}\ell_{s}^{2}} & 0\\
0 & 0 & \lambda & -1\\
-\frac{k}{m_{B}\ell_{s}^{2}} & 0 & -\frac{k}{m_{B}\ell_{s}^{2}} & \lambda\end{vmatrix}\\
0 & = & -\frac{k\lambda^{2}}{\ell_{s}^{2}m_{B}}+\frac{k\lambda^{2}}{\ell_{s}^{2}m_{m}}+\lambda^{4}\end{eqnarray*}

\end_inset


\begin_inset Formula \begin{eqnarray*}
\lambda_{1} & = & 0\\
\lambda_{2} & = & 0\\
\lambda_{3} & = & -\sqrt{\frac{k}{\ell_{s}^{2}m_{B}}-\frac{k}{\ell_{s}^{2}m_{m}}}\\
\lambda_{4} & = & \sqrt{\frac{k}{\ell_{s}^{2}m_{B}}-\frac{k}{\ell_{s}^{2}m_{m}}}\end{eqnarray*}

\end_inset


\begin_inset Formula $\lambda_{4}>0$
\end_inset

 
\begin_inset Formula $\rightarrow$
\end_inset

The system is not stable.
\end_layout

