\begin_layout Subsection
Linearization about 
\begin_inset Formula $\left(0,0\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
\dot{x}_{1} & = & x_{2}\\
\dot{x}_{2} & = & \frac{1}{m_{m}}F-\frac{2k\sqrt{1-\frac{\left(\ell_{0}-x_{1}+x_{3}\right)^{2}}{4\ell_{S}^{2}}}\left(\arccos\left(\frac{\left(\ell_{0}-x_{1}+x_{3}\right)}{2\ell_{S}}\right)-\arccos\left(\frac{\ell_{0}}{2\ell_{S}}\right)\right)}{m_{m}\ell_{S}}\\
\dot{x}_{3} & = & x_{4}\\
\dot{x}_{4} & = & \frac{2k\sqrt{1-\frac{\left(\ell_{0}-x_{1}+x_{3}\right)^{2}}{4\ell_{S}^{2}}}\left(\arccos\left(\frac{\left(\ell_{0}-x_{1}+x_{3}\right)}{2\ell_{S}}\right)-\arccos\left(\frac{\ell_{0}}{2\ell_{S}}\right)\right)}{m_{B}\ell_{S}}-g\end{eqnarray*}

\end_inset


\begin_inset Formula \begin{eqnarray*}
J & = & \begin{bmatrix}\frac{\partial\dot{x}_{1}}{\partial x_{1}} & \frac{\partial\dot{x}_{1}}{\partial x_{2}} & \frac{\partial\dot{x}_{1}}{\partial x_{3}} & \frac{\partial\dot{x}_{1}}{\partial x_{4}}\\
\frac{\partial\dot{x}_{2}}{\partial x_{1}} & \frac{\partial\dot{x}_{2}}{\partial x_{2}} & \frac{\partial\dot{x}_{2}}{\partial x_{3}} & \frac{\partial\dot{x}_{2}}{\partial x_{4}}\\
\frac{\partial\dot{x}_{3}}{\partial x_{1}} & \frac{\partial\dot{x}_{3}}{\partial x_{2}} & \frac{\partial\dot{x}_{3}}{\partial x_{3}} & \frac{\partial\dot{x}_{3}}{\partial x_{4}}\\
\frac{\partial\dot{x}_{4}}{\partial x_{1}} & \frac{\partial\dot{x}_{4}}{\partial x_{2}} & \frac{\partial\dot{x}_{4}}{\partial x_{3}} & \frac{\partial\dot{x}_{4}}{\partial x_{4}}\end{bmatrix}_{\left(0,0,0.0\right)}\\
 & = & \begin{bmatrix}0 & 1 & 0 & 0\\
\frac{\partial\dot{x}_{2}}{\partial x_{1}} & 0 & \frac{\partial\dot{x}_{2}}{\partial x_{3}} & 0\\
0 & 0 & 0 & 1\\
\frac{\partial\dot{x}_{4}}{\partial x_{1}} & 0 & \frac{\partial\dot{x}_{4}}{\partial x_{3}} & 0\end{bmatrix}_{\left(0,0,0.0\right)}\end{eqnarray*}

\end_inset


\begin_inset Formula \begin{eqnarray*}
\frac{\partial\dot{x}_{2}}{\partial x_{1}} & = & -\frac{k}{m_{m}\ell_{s}^{2}}-\frac{k\left(\ell_{0}-x_{1}-x_{3}\right)\left(\arccos\left(-\frac{-\ell_{0}+x_{1}+x_{3}}{2\ell_{s}}\right)-\arccos\left(\frac{\ell_{0}}{2\ell_{s}}\right)\right)}{2\ell_{s}^{3}m_{m}\sqrt{1-\frac{\left(-\ell_{0}+x_{1}+x_{3}\right)^{2}}{4\ell_{s}^{2}}}}\\
\left.\frac{\partial\dot{x}_{2}}{\partial x_{1}}\right|_{\left(0,0,0.0\right)} & = & -\frac{k}{m_{m}\ell_{s}^{2}}\end{eqnarray*}

\end_inset


\begin_inset Formula \begin{eqnarray*}
\frac{\partial\dot{x}_{2}}{\partial x_{3}} & = & -\frac{k}{m_{m}\ell_{s}^{2}}-\frac{k\left(\ell_{0}-x_{1}-x_{3}\right)\left(\arccos\left(-\frac{-\ell_{0}+x_{1}+x_{3}}{2\ell_{s}}\right)-\arccos\left(\frac{\ell_{0}}{2\ell_{s}}\right)\right)}{2\ell_{s}^{3}m_{m}\sqrt{1-\frac{\left(-\ell_{0}+x_{1}+x_{3}\right)^{2}}{4\ell_{s}^{2}}}}\\
\left.\frac{\partial\dot{x}_{2}}{\partial x_{3}}\right|_{\left(0,0,0.0\right)} & = & -\frac{k}{m_{m}\ell_{s}^{2}}\end{eqnarray*}

\end_inset


\begin_inset Formula \begin{eqnarray*}
\frac{\partial\dot{x}_{4}}{\partial x_{1}} & = & \frac{k}{m_{B}\ell_{s}^{2}}+\frac{k\left(\ell_{0}-x_{1}-x_{3}\right)\left(\arccos\left(-\frac{-\ell_{0}+x_{1}+x_{3}}{2\ell_{s}}\right)-\arccos\left(\frac{\ell_{0}}{2\ell_{s}}\right)\right)}{2\ell_{s}^{3}m_{B}\sqrt{1-\frac{\left(-\ell_{0}+x_{1}+x_{3}\right)^{2}}{4\ell_{s}^{2}}}}\\
\left.\frac{\partial\dot{x}_{4}}{\partial x_{1}}\right|_{\left(0,0,0.0\right)} & = & \frac{k}{m_{B}\ell_{s}^{2}}\end{eqnarray*}

\end_inset


\begin_inset Formula \begin{eqnarray*}
\frac{\partial\dot{x}_{4}}{\partial x_{3}} & = & \frac{k}{m_{B}\ell_{s}^{2}}+\frac{k\left(\ell_{0}-x_{1}-x_{3}\right)\left(\arccos\left(-\frac{-\ell_{0}+x_{1}+x_{3}}{2\ell_{s}}\right)-\arccos\left(\frac{\ell_{0}}{2\ell_{s}}\right)\right)}{2\ell_{s}^{3}m_{B}\sqrt{1-\frac{\left(-\ell_{0}+x_{1}+x_{3}\right)^{2}}{4\ell_{s}^{2}}}}\\
\left.\frac{\partial\dot{x}_{4}}{\partial x_{3}}\right|_{\left(0,0,0.0\right)} & = & \frac{k}{m_{B}\ell_{s}^{2}}\end{eqnarray*}

\end_inset


