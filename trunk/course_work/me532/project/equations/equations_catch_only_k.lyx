\begin_layout Standard
Take the Laplace:
\begin_inset Formula \begin{eqnarray}
m_{L}\left[s^{2}X_{L}(s)-sx_{L0}-\dot{x}_{L0}\right] & = & k\left[X_{m}(s)-X_{L}(s)\right]\nonumber \\
m_{L}s^{2}X_{L}(s)-m_{L}x_{L0}s-m_{L}\dot{x}_{L0} & = & kX_{m}(s)-kX_{L}(s)\nonumber \\
\nonumber \\\textrm{Move }X_{m}(s)\textrm{ to the left side:}\nonumber \\
X_{m}(s)k & = & m_{L}s^{2}X_{L}(s)+kX_{L}(s)-m_{L}x_{L0}s-m_{L}\dot{x}_{L0}\nonumber \\
X_{m}(s)k & = & X_{L}(s)\left(m_{L}s^{2}+k\right)-m_{L}x_{L0}s-m_{L}\dot{x}_{L0}\nonumber \\
X_{m}(s) & = & \frac{X_{L}(s)\left(m_{L}s^{2}+k\right)-m_{L}x_{L0}s-m_{L}\dot{x}_{L0}}{k}\label{eq:sys2}\end{eqnarray}

\end_inset


\begin_inset Formula \begin{eqnarray*}
