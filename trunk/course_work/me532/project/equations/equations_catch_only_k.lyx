\begin_layout Standard
System 1 (motor side):
\begin_inset Formula \begin{eqnarray*}
\Sigma F=m_{m}\ddot{x}_{m} & = & F_{m}-F_{k}\\
m_{m}\ddot{x}_{m}(t) & = & F_{m}(t)-k[x_{m}(t)-x_{L}(t)]\\
m_{m}\ddot{x}_{m}(t)+kx_{m}(t) & = & F_{m}(t)+kx_{L}(t)\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
Take the Laplace:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray}
m_{m}\left[s^{2}X_{m}(s)-sx_{m0}-\dot{x}_{m0}\right] & = & F_{m}(s)-k\left[X_{m}(s)-X_{L}(s)\right]\nonumber \\
m_{m}s^{2}X_{m}(s)-m_{m}x_{m0}s-m_{m}\dot{x}_{m0} & = & F_{m}(s)-kX_{m}(s)+kX_{L}(s)\nonumber \\
\nonumber \\\textrm{Move }X_{m}(s)\textrm{ to the left side:}\nonumber \\
m_{m}s^{s}X_{m}(s)+kX_{m}(s) & = & F_{m}(s)+X_{L}(s)k+m_{m}x_{m0}s+m_{m}\dot{x}_{m0}\nonumber \\
X_{m}(s)\left[m_{m}s^{2}+k\right] & = & F_{m}(s)+X_{L}(s)k+m_{m}x_{m0}s+m_{m}\dot{x}_{m0}\nonumber \\
X_{m}(s) & = & \frac{F_{m}(s)+X_{L}(s)k+m_{m}x_{m0}s+m_{m}\dot{x}_{m0}}{m_{m}s^{2}+k}\label{eq:sys1}\end{eqnarray}

