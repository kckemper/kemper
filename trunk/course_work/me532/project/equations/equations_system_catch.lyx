\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
\ddot{x}_{m}(t) & = & \frac{1}{m_{m}}F_{m}(t)-\frac{k}{m_{m}}x_{m}(t)+\frac{k}{m_{m}}x_{L}(t)-\frac{B}{m_{m}}\dot{x}_{m}(t)+\frac{B}{m_{m}}\dot{x}_{L}(t)\\
\ddot{x}_{L}(t) & = & \frac{k}{m_{L}}x_{m}(t)-\frac{k}{m_{L}}x_{L}(t)+\frac{B}{m_{L}}\dot{x}_{m}(t)-\frac{B}{m_{L}}\dot{x}_{L}(t)\end{eqnarray*}

\end_inset


\begin_inset Formula \begin{eqnarray*}
x_{1} & = & x_{m}\\
