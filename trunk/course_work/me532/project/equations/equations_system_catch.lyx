\begin_layout Standard
System 1 (motor side):
\begin_inset Formula \begin{eqnarray*}
\Sigma F=m_{m}\ddot{x}_{m} & = & F_{m}-F_{k}-F_{B}\\
m_{m}\ddot{x}_{m}(t) & = & F_{m}(t)-k\left(x_{m}(t)-x_{L}(t)\right)-B\left(\dot{x}_{m}(t)-\dot{x}_{L}(t)\right)\\
m_{m}\ddot{x}_{m}(t) & = & F_{m}(t)-kx_{m}(t)+kx_{L}(t)-B\dot{x}_{m}(t)+B\dot{x}_{L}(t)\\
\ddot{x}_{m}(t) & = & \frac{1}{m_{m}}F_{m}(t)-\frac{k}{m_{m}}x_{m}(t)+\frac{k}{m_{m}}x_{L}(t)-\frac{B}{m_{m}}\dot{x}_{m}(t)+\frac{B}{m_{m}}\dot{x}_{L}(t)\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
System 2 (load side):
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
\Sigma F=m_{L}\ddot{x}_{L} & = & F_{k}+F_{B}\\
m_{L}\ddot{x}_{L} & = & k\left(x_{m}(t)-x_{L}(t)\right)+B\left(\dot{x}_{m}(t)-\dot{x}_{L}(t)\right)\\
m_{L}\ddot{x}_{L} & = & x_{m}(t)k-x_{L}(t)k+\dot{x}_{m}(t)B-\dot{x}_{L}(t)B\\
\ddot{x}_{L}(t) & = & \frac{k}{m_{L}}x_{m}(t)-\frac{k}{m_{L}}x_{L}(t)+\frac{B}{m_{L}}\dot{x}_{m}(t)-\frac{B}{m_{L}}\dot{x}_{L}(t)\end{eqnarray*}

\end_inset


\end_layout

